---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <subtitle>{{ site.tagline }}</subtitle>
 <category term="semantic web" label="Semantic Web"/>
 <category term="technology" label="Technology"/>
 <category term="development" label="Development"/>
 <link href="{{ site.url }}/atom.xml" rel="self"/>
 <link href="{{ site.url }}/"/>
 <icon>{{ site.url }}/public/favicon.ico</icon>
 <logo>{{ site.url }}/public/apple-touch-icon-precomposed.png</logo>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author.name }}</name>
   <email>{{ site.author.email }}</email>
 </author>
 <rights type="html">
    &#x3C;span resource=&#x22;http://depressiverobot.com/&#x22;&#x3E;All content on this site by &#x3C;span property=&#x22;http://creativecommons.org/ns#attributionName&#x22;&#x3E;Marvin Frommhold&#x3C;/span&#x3E; is licensed under a &#x3C;a property=&#x22;http://creativecommons.org/ns#license&#x22; href=&#x22;http://creativecommons.org/licenses/by-nc/4.0/&#x22;&#x3E;Creative Commons Attribution-NonCommercial 4.0 International License&#x3C;/a&#x3E;.&#x3C;/span&#x3E;
  </rights>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ site.url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
